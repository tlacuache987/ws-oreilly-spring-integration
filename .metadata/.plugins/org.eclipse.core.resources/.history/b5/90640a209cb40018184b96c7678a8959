package com.example.demo.service.impl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.example.demo.gateway.RedisChannelOutboundGateway;
import com.example.demo.model.Post;
import com.example.demo.service.ConsumerService;

@Service
public class RedisConsumerService implements ConsumerService {

	private RedisChannelOutboundGateway redisChannelOutboundGateway;

	@Autowired
	public RedisConsumerService(RedisChannelOutboundGateway redisChannelOutboundGateway) {
		this.redisChannelOutboundGateway = redisChannelOutboundGateway;
	}

	@Override
	public void enqueue(Post post) {
		redisChannelOutboundGateway.enqueue(post);
	}

}

package com.example.demo.util;

public enum Color {
	BLACK("black", "\033[0m"), BLUE("blue"), CYAN("cyan"), DEFAULT("default"), GREEN(
			"green"),
	MAGENTA("magenta"), RED("red", "\033[31m"), WHITE("white"), YELLOW(
			"yellow");

	private String color;

	private String colorCode;

	private Color(String color, String colorCode) {
		this.color = color;
		this.colorCode = colorCode;
	}

	public String getColor() {
		return this.color;
	}
}
